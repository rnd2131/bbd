{
    "version": 2,
    "name": "bbd",
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "pages/*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.css",
        "use": "@vercel/static"
      },
      {
        "src": "*.js",
        "use": "@vercel/static"
      },
      {
        "src": "*.json",
        "use": "@vercel/static"
      },
      {
        "src": "*.png",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/system",
        "dest": "/system.html"
      },
      {
        "src": "/pages/about",
        "dest": "/pages/about.html"
      },
      {
        "src": "/pages/contact",
        "dest": "/pages/contact.html"
      },
      {
        "src": "/pages/services",
        "dest": "/pages/services.html"
      }
    ]
  }
  